{{# patterninfo }}<p class="mbn"><code>Tilganger</code></p>{{/ patterninfo }}
{{# tilganger }}
    <div class="action-line dark-text hidden-on-print {{ styleModifier }}">
        <div class="align_left" style="margin-top: 12px;">
            {{# sokeFilter }} 
                <label class="form-element__label" style="color: white">Hvem skal få ny tilgang?</label>
                {{> atoms-filterbox-valg:mbs }}
                {{> molecules-sokefilter-avkrysningsbokser:mbl }}
            {{/ sokeFilter }}
        </div>
        <div class="align_left" style="margin-left: 12px; margin-top: 12px;">
            {{# nedtrekksliste }}
                <label class="form-element__label" style="color: white">Rolle</label>
                {{> atoms-nedtrekksliste:mbs }}
            {{/ nedtrekksliste }}
        </div>
        <div class="align_left" style="margin-left: 12px; margin-top: 12px;">
            {{# sokeFilter2 }}
