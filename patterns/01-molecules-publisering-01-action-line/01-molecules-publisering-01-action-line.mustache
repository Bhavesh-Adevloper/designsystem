{{# patterninfo }}<p class="mbn"><code>kladd ikke lagret:</code></p>{{/ patterninfo }}
{{# kladdIkkeLagret }}
    <div class="action-line hidden-on-print {{ styleModifier }}">
        <div class="align_left">
            <div class="action-line__status">KLADD</div>
        </div>
        <div class="align_right">
            <span class="action-line__preview-link action-line__preview-link--disabled">
                <span class="icon-preview--silver grunt-icon grunt-icon--32"></span>
                Forhåndsvis
            </span>
            <button class="button button--success" disabled>Lagre kladd</button>
            <button class="button" disabled>Publiser</button>
        </div>
    </div>
{{/ kladdIkkeLagret }}
{{# patterninfo }}<p class="mbn"><code>kladd lagret:</code></p>{{/ patterninfo }}
{{# kladdLagret }}
    <div class="action-line hidden-on-print {{ styleModifier }}">
        <div class="align_left">
            <div class="action-line__status">KLADD</div>
        </div>
        {{# varsel}}
            <div>
                <div class="action-line__warning">{{varsel}}</div>
            </div>
        {{/ varsel}}
        <div class="align_right">
            {{#url}}
            <a href="{{ url }}" class="action-line__preview-link">
                <span class="icon-preview grunt-icon grunt-icon--32"></span>
                Forhåndsvis</a>
            {{/url}}
            <button class="button button--success active">Lagre kladd</button>
            <button class="button">Publiser</button>
        </div>
    </div>
{{/ kladdLagret }}
