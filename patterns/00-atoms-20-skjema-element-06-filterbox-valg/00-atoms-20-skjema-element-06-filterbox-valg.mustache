<div name="sok" class="filter-search no_max_width {{ styleModifier }}">
    <label for="filter_search" class="filter-search__label">
        <span class="icon-searchglass--small grunt-icon grunt-icon--32"></span>
        <span class="screen-reader-text">{{ label }}</span>
    </label>
    <input type="search" id="filter_search" placeholder="{{ placeholder }}" class="js_filter_search_input filter-search__input mbn" />
    <button class="filter-search__clear-search-button js_clear_search" style="display: none;">
        <span class="icon-close--small grunt-icon grunt-icon--32"></span>
        <span class="screen-reader-text">Fjern s√∏keord</span>
    </button>
    <div class="js_filter_search_result filter-search__result">
        <ul>
            {{# enkelliste }}
                <li>
                    <a href="#">
                        {{# icon }}
                        <span class="icon-{{icon}} grunt-icon grunt-icon--32"></span>{{{ tittel }}}<span>{{{ test }}}</span>
                        {{/ icon}}
                        {{^ icon }}
                            <span class="grunt-icon grunt-icon--32"></span>{{{ tittel }}}<span>{{{ test }}}</span>
                        {{/ icon}}
                    </a>
                
                </li>
            {{/ enkelliste }}
        </ul>
    </div>
</div>

