<div class="search-checkbox-filter {{styleModifier}}">
    <button class="js_search-checkbox-filter__toggle search-checkbox-filter__button">Avgrens søket</button>
    <div class="search-checkbox-filter__content">

        {{# avkryssingsbokser }}
            <fieldset class="search-checkbox-filter__fieldset mbml search-checkbox-filter__fieldset--open">
                <legend>
                    <a href="#" class="js_search-checkbox-filter__title--advanced search-checkbox-filter__title search-checkbox-filter__title--advanced" title="Åpne {{ legend }} filter">
                    {{ legend }}
                    </a>
                </legend>
                <div class="search-checkbox-filter__group">
                    {{> atoms-avkryssingsboks }}
                </div>
            </fieldset>
        {{/ avkryssingsbokser }}

        {{# radioknapper }}
            <fieldset class="search-checkbox-filter__fieldset mbml search-checkbox-filter__fieldset--open">
                <legend>
                    <a href="#" class="js_search-checkbox-filter__title--advanced search-checkbox-filter__title search-checkbox-filter__title--advanced" title="Åpne {{ legend }} filter">
                    {{ legend }}
                    </a>
                </legend>
                <div class="search-checkbox-filter__group">
                    
                    {{> atoms-radioknapp }}
                    
                    {{# omRadioknapperLink }}
                        <p class="{{styleModifier}}">
                            <a href="{{ url }}" title="{{ tittel }}" class="read_more_link">{{ tittel }}</a>
                        </p>
                    {{/ omRadioknapperLink }}
                </div>
            </fieldset>
        {{/ radioknapper }}

        {{# nullstillFiltre }}
        <div class="mbml">
            <a class="search-checkbox-filter__reset-link" href="#">Nullstill filtre</a>
        </div>
        {{/ nullstillFiltre }}
    </div>
    <a href="#" class="js_search-checkbox-filter__toggle search-checkbox-filter__close-button">Lukk</a>
</div>
