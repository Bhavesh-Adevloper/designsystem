{{> organisms-header-varianter }}
{{> organisms-brodsmuler }}
<main id="innhold" tabindex="-1" class="smMa desktop_centered">

    <div class="page_header_container_with_org_menu">
        {{> molecules-sideoverskrift-h2-med-lenke(sidetittel: "{{ organisasjonsenhet.tittel }}", url: "{{ organisasjonsenhet.url }}") }}
    </div>

    <div class="row">
        <div class="col_md_12 col_lg_4">

            {{> molecules-org-meny }}

        </div>
        <div class="col_md_12 col_lg_8 org_main_col">
            
            {{# vanligLinkListe }}
                <div class="mbj">
                    {{> molecules-moduloverskrift-h1:mbs }}
                    {{> molecules-link-liste:mbj }}
                </div>
             {{/ vanligLinkListe }}
            
            {{# linkListeMedFilterSoek }}
                    
                <!-- Hvis listen inneholder skoler og barnehager -->
                {{> molecules-moduloverskrift-h1:mbml }}

                {{# searchFilter }}
                    {{> atoms-filterboks }}
                {{/ searchFilter }}

                {{# ellerVisAlle }}
                <p class="filter_shortcut_container">
                    Eller vis alle: <a href="{{ link.pages-alle-skoler }}" title="skoler">skoler</a>, <a href="{{ link.pages-alle-skoler }}" title="barnehager">barnehager</a>
                </p>
                {{/ ellerVisAlle }}

                <p class="js_empty_filter_search no_results" style="display: none;">
                    Ingenting funnet
                </p>

                {{> molecules-link-liste-filter-soek:mbj }}
                
            {{/ linkListeMedFilterSoek }}

        </div>
    </div>
</main>
{{> organisms-footer }}