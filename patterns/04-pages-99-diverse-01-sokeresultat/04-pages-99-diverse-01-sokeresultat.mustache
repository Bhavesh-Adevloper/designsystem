        <a href="javascript:lastFlere()" id="js_lastflere" class="load_more_link" style="display:none;">
            <span class="read_more_link read_more_link--20">
                Vis flere treff <!-- (<span id="js_antallflere"></span>)  -->
            </span>
        </a>

        <script>
            function lastFlere() {
                var start = $("#js_search_results").children().length;
                jQuery.get(
                        "/sok/flere",
                        {start: start, q: 'bibliotek', innholdstype: ''},
                        function(data) {
                            $("#js_search_results").append(data);
                            visLastFlere();
                        }
                );
            }

            function visLastFlere() {
                var visteTreff = $("#js_search_results").children().length;
                var antallFlere = 484 - visteTreff;
                $("#js_antallflere").html(antallFlere);

                if (antallFlere > 0) {
                    $("#js_lastflere").show();
                } else {
                    $("#js_lastflere").hide();
                }
            }

            $(function() { visLastFlere(); });
        </script>
