<section class="service_shortcut_container {{ styleModifier }}">
    <a href="{{ serviceSnarvei.url }}" title="{{ serviceSnarvei.tittel }}" class="service_icon {{ serviceSnarvei.storIkon }}">
        <span class="screen-reader-text">{{ serviceSnarvei.tittel }}</span>
    </a>
    <h3 class="service_shortcut_title">
        {{ serviceSnarvei.tittel }}
    </h3>
    <p class="service_shortcut_text">{{ serviceSnarvei.tekst }}</p>
    <p class="service_shortcut_link_container">
        <a href="{{ serviceSnarvei.url }}" title="{{ serviceSnarvei.tittel }}" class="button large">{{ serviceSnarvei.knappTittel }}</a>
    </p>
</section>

