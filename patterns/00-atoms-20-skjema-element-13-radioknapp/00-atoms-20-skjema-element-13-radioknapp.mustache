<fieldset>
    <legend class="{{#skjultTittel}}screen-reader-text{{/skjultTittel}} {{styleModifier}}">{{legend}}</legend>
    {{#radio}}
    <div class="form-element {{styleModifier}}">
        <input class="form-element__radio" type="radio" name="{{legend}}" id="{{name}}" {{#checked}}checked{{/checked}} >
        <label class="form-element__label" for="{{name}}">{{label}}</label>
    </div>
    {{/radio}}
</fieldset>