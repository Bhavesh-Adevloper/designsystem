<div class="sak {{ styleModifier }}">
    {{# imgLandscape }}
        <div class="excerpt_image_container_ruu">
            <a href="{{ link.pages-luftkvalitet }}" title="{{ tittel }}" class="image_link">
                <img src="{{ imgLandscape.src }}" alt="{{ imgLandscape.alt }}">
            </a>
        </div>
    {{/ imgLandscape }}
    <div class="excerpt_story_container_ruu">
        <div class="sakstittel_ruu">
            <h3 class="teaser_title">
                <a href="#">
                    <span class="read_more_link_large">
                        {{ tittel }}
                    </span>
                </a>
            </h3>
        </div>
        {{# ingress }}
        <p class="story_excerpt">		
            {{ ingress }}
        </p>
        {{/ ingress }}
    </div>
</div>