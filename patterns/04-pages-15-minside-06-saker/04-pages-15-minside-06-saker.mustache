{{> organisms-header-varianter }}
{{> organisms-brodsmuler }}
<main id="innhold" tabindex="-1" class="smMa desktop_centered"> 

    <div class="page_header_container_with_org_menu">
        
        {{# forInnbygger }}
        {{> molecules-sideoverskrift-h2-med-lenke(sidetittel: "{{ innbygger.tittel }}", url: "{{ innbygger.url }}") }}
        {{/ forInnbygger }}
        
        {{# forOrganisasjon }}
        {{> molecules-sideoverskrift-h2-med-lenke(sidetittel: "{{ organisasjon.tittel }}", url: "{{ organisasjon.url }}") }}
        {{/ forOrganisasjon }}
        
    </div>

    <div class="row">
        <div class="col_md_12 col_lg_4">

            {{# meny }}
            {{> molecules-org-meny }}
            {{/ meny }}

        </div>
        <div class="col_md_12 col_lg_8 org_main_col">

            {{> molecules-moduloverskrift-h1:mbm(tittel: "Saker") }}

            <p>Listen viser byggesaker og noen andre sakstyper. Du kan ha saker med kommunen som ikke vises i denne oversikten.</p>

            <div class="case_search_container">
                <form action="#">
                    <input type="search" class="search_input" accesskey="4" placeholder="Søk etter saker">
                    <button><span class="icon searchglass_icon"><span class="screen-reader-text">Søk</span></span></button>
                </form>
            </div>

            {{> molecules-saker-liste:mbj }}
            {{# lastFlereSaker }}
            {{> molecules-last-inn-flere-sentrert:neg-mtxl }}
            {{/ lastFlereSaker }}

            {{> molecules-tilbakemelding }}

        </div>
    </div>
   
</main>
{{> organisms-footer }}