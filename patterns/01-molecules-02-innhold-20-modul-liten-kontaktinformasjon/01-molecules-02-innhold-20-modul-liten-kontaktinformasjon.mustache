<div class="module_container {{ styleModifier }}">
    {{# bilde }}
        <div class="module_image_container">
            <img src="{{ imgLandscape.src }}" alt="{{ imgLandscape.alt }}" class="module_image"/>
        </div>
    {{/ bilde }}
    <div class="module_content_container module_content_container--contact-information">
        <div class="module_content_excerpt">
            <dl class="contact_content">
                <dt class="screen-reader-text">Adresse:</dt>
                <dd>
                   Starhusgaten 2A, 5014 Bergen
                </dd>
                <dt class="screen-reader-text">Telefon:</dt>
                <dd class="with_icon">
                    <a href="tel:55566000" title="Ring oss">
                        <span class="icon phone_icon medium_size"></span>
                        55 56 60 00
                    </a>
                </dd>
            </dl>
        </div>
    </div>
</div>